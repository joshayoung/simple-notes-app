<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SimpleNotes.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="One">#5EA8A6</Color> 
            <Color x:Key="Two">#7DF5F1</Color>
            <Color x:Key="Three">#F59371</Color>
            <Color x:Key="Four">#3DA87D</Color>
            <Color x:Key="Five">#E9EEEC</Color>
            <Style x:Key="NoteButton" TargetType="Button">
                <Setter Property="BackgroundColor" Value="{StaticResource Four}" />
                <Setter Property="TextColor" Value="#FFFFFF" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>